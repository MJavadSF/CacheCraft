{
  "name": "cache-craft-engine",
  "version": "0.1.2",
  "description": "A lightweight, fully client-side IndexedDB cache engine with compression, encoding, LRU eviction, TTL and stale-while-revalidate support",
  "author": {
    "name": "Mohammad Javad",
    "email": "mjavadsf72@gmail.com",
    "url": "https://github.com/MJavadSF"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && rollup -c",
    "prepublishOnly": "npm run build",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "keywords": [
    "cache",
    "indexeddb",
    "client-side",
    "offline",
    "compression",
    "lru",
    "stale-while-revalidate",
    "browser-cache",
    "web-storage",
    "performance",
    "pwa",
    "offline-first",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MJavadSF/CacheCraft.git"
  },
  "bugs": {
    "url": "https://github.com/MJavadSF/CacheCraft/issues"
  },
  "homepage": "https://github.com/MJavadSF/CacheCraft#readme",
  "funding": {
    "type": "individual",
    "url": "https://reymit.ir/mjavadsf"
  },
  "contributors": [
    {
      "name": "MJ. Soleymani Fard",
      "email": "mjavadsf72@gmail.com",
      "url": "https://github.com/MJavadSF"
    },
    {
      "name": "Abbas Sourani",
      "email": "abbas.sourani.smartis@gmail.com",
      "url": "https://github.com/abbassius"
    }
  ],
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.3.0",
    "rollup": "^4.57.1",
    "rollup-plugin-dts": "^6.3.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  }
}